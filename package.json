{
    "name": "ScratchJr",
    "productName": "ScratchJr",
    "version": "1.3.2",
    "description": "ScratchJr Desktop Edition",
    "main": "src/main.js",
    "scripts": {
        "start": "electron-forge start",
        "debugMain": "electron-forge start -i",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "make64": "electron-forge make --arch=x64",
        "make32": "electron-forge make --arch=ia32",
        "makeAll": "electron-forge make --arch=ia32,x64",
        "publish": "electron-forge publish",
        "lint": "eslint src --color"
    },
    "keywords": [],
    "author": "ScratchJr - Open Source Community Port",
    "license": "MIT",
    "app-copyright": "Copyright (c) 2016, MIT",
    "config": {
        "forge": "./forge.config.js"
    },
    "eslintConfig": {
        "extends": "eslint-config-airbnb",
        "parserOptions": {
            "ecmaVersion": 6,
            "sourceType": "module",
            "ecmaFeatures": {}
        },
        "rules": {
            "no-use-before-define": 0,
            "max-len": 0,
            "no-multi-spaces": 0,
            "no-param-reassign": 0,
            "indent": 0,
            "padded-blocks": 0,
            "class-methods-use-this": 0,
            "trailing-spaces-not-allowed": 0,
            "vars-on-top": 0,
            "no-trailing-spaces": 0,
            "no-multiple-empty-lines": 0,
            "no-continue": 0,
            "no-plusplus": 0,
            "spaced-comment": 0,
            "no-var": 0,
            "eqeqeq": 0,
            "prefer-template": 0,
            "camelcase": 0,
            "space-before-blocks": 0,
            "quote-props": 0,
            "quotes": [
                "error",
                "single",
                {
                    "allowTemplateLiterals": true
                }
            ],
            "comma-dangle": 0,
            "default-case": 0,
            "no-useless-escape": 0,
            "yoda": 0,
            "no-useless-return": 0,
            "space-before-function-paren": 0,
            "one-var-declaration-per-line": 0,
            "object-curly-spacing": 0,
            "prefer-const": 0,
            "func-call-spacing": 0,
            "no-tabs": 0,
            "func-names": 0,
            "prefer-arrow-callback": 0,
            "no-mixed-spaces-and-tabs": 0,
            "no-mixed-operators": 0,
            "no-unused-vars": [
                2,
                {
                    "args": "after-used",
                    "varsIgnorePattern": "^_"
                }
            ],
            "no-restricted-syntax": 0,
            "no-nested-ternary": 0,
            "import/extensions": 0,
            "no-useless-concat": 0,
            "no-unneeded-ternary": [
                "error",
                {
                    "defaultAssignment": true
                }
            ],
            "one-var": 0,
            "object-shorthand": 0,
            "operator-assignment": 0,
            "no-lonely-if": 0,
            "no-else-return": 0,
            "no-shadow": 0,
            "no-undef-init": 0,
            "radix": 0,
            "no-bitwise": 0,
            "no-restricted-properties": [
                0,
                {
                    "object": "Math",
                    "property": "pow",
                    "message": "Use the exponentiation operator (**) instead."
                }
            ],
            "guard-for-in": 0
        },
        "globals": {
            "AndroidInterface": true,
            "window": true,
            "WebKitCSSMatrix": true,
            "webkitAudioContext": true,
            "electron": true,
            "require": true
        },
        "env": {
            "browser": true,
            "node": true
        },
        "eslintIgnore": [
            "./src/app/src/snap/snap.svg-min.js"
        ]
    },
    "dependencies": {
        "electron-compile": "^6.4.4",
        "electron-is-dev": "^1.2.0",
        "electron-squirrel-startup": "^1.0.0",
        "intl": "^1.2.5",
        "intl-messageformat": "^2.2.0",
        "jszip": "^3.5.0",
        "snapsvg": "^0.5.1",
        "sql.js": "^0.4.0",
        "hoek": "^6.1.3"
    },
    "devDependencies": {
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "electron-forge": "^5.2.4",
        "electron-prebuilt-compile": "8.2.0",
        "eslint": "^7.4.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-react": "^7.20.3"
    }
}
